{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-unused-vars": 0,
    "prefer-const": "warn",
    "no-var": "error",
    "no-undef": "off",
    "require-jsdoc":"off"

  },    
  "overrides": [
    {
        "files": ["*.js", "*.jsx"],
        "rules": {
            "no-dupe-class-members": "off"
        }
    }
]
}
